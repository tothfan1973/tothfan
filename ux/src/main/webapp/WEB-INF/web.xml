<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>AppFormService</servlet-name>
        <servlet-class>com.ux.server.AppFormServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AppFormService</servlet-name>
        <url-pattern>/ux/app</url-pattern>
    </servlet-mapping>

    <listener>
        <listener-class>com.ux.common.ContextListener</listener-class>
    </listener>

    <welcome-file-list>
        <welcome-file>/ux.html</welcome-file>
    </welcome-file-list>

</web-app>

